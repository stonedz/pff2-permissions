{
	"name": "stonedz/pff2-permissions",
	"type": "pff2-module",
	"description": "Manages permissions in pff2 controllers",
	"license": "MIT",
	"minimum-stability": "dev",
	"keywords": [
		"pff2",
		"module",
		"permissions"
	],
	"authors": [
		{
			"name": "Paolo Fagni",
			"email": "paolo.fagni@gmail.com",
			"role": "Developer"
		}
	],
	"homepage": "https://github.com/stonedz/pff2-permissions",
	"require": {
		"stonedz/pff2-installers": "*"
	},
	"require-dev": {
		"stonedz/pff2": "^4",
		"phpunit/phpunit": "^10.5"
	},
	"autoload": {
		"psr-4": {
			"pff\\modules\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/",
			"pff\\models\\": "tests/Fixtures/Models/"
		}
	},
	"config": {
		"allow-plugins": {
			"stonedz/pff2-installers": true
		}
	}
}
